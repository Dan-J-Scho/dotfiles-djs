[alias]
	st = status
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	co = checkout
	cob = checkout -b
	ci = commit -m
	b = branch
	com = checkout master
	aa = add -A
	d = diff
[user]
	name = dan schofield
	email = daniel.schofield1@nhs.net
[color]
	ui = true
[push]
	default = upstream
[core]
	editor = /usr/local/bin/vim
[credential]
	helper = osxkeychain
[github]
	user = Dan-J-Scho
[ghi]
	token = !security find-internet-password -a Dan-J-Scho -s github.com -l 'ghi token' -w
[url "https://github.com/"]
	insteadOf = git@github.com:
[url "https://"]
	insteadOf = git://
